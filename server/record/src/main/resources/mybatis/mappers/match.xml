<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dkslgg.record.model.dao.MatchDao">
    <insert id="insert" parameterType="matchVo">
        INSERT IGNORE INTO match(`match_id`, `game_creation`, `game_duration`)
               VALUES(#{matchId}, #{gameCreation}, #{gameDuration});
    </insert>
</mapper>