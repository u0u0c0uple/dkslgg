<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dkslgg.record.model.dao.SummonerDao">
    <select id="selectPuuidByGameNameAndTagId" parameterType="String">
        SELECT `puuid` FROM SUMMONER WHERE `game_name` = #{gameName} AND `tag_line` = #{tagLine}
    </select>
</mapper>